    <item android:state_enabled="false">
        <shape android:shape="rectangle"  >
            <corners android:radius="50dip" />
            <stroke android:width="1dip" android:color="@color/transparent"/>
            <solid android:color="@color/md_blue_400"/>
            <padding
                android:left="8dp"
                android:bottom="4dp"
                android:right="8dp"
                android:top="4dp"/>
        </shape>
    </item>

    <item android:state_enabled="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="50dip" />
            <stroke android:width="1dip" android:color="@color/transparent" />
            <solid android:color="@color/md_blue_400"/>
            <padding
                android:left="8dp"
                android:bottom="4dp"
                android:right="8dp"
                android:top="4dp"/>
        </shape>
    </item>


